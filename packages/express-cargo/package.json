{
    "name": "express-cargo",
    "version": "0.0.0",
    "description": "express middleware for class-based request parsing",
    "main": "./dist/src/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "development": "./src/index.ts",
            "import": "./dist/index.esm.js",
            "require": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc --emitDeclarationOnly --declaration --outDir dist && swc src -d dist",
        "lint": "eslint src --ext .ts",
        "test": "jest"
    },
    "keywords": [
        "express",
        "middleware",
        "cargo",
        "request"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@swc/cli": "^0.3.10",
        "@swc/core": "^1.4.6",
        "@swc/jest": "^0.2.38",
        "@types/node": "^20.11.19",
        "jest": "^30.0.4"
    },
    "peerDependencies": {
        "@types/express": ">=4.17.0 <6",
        "express": ">=4.17.0 <6",
        "reflect-metadata": "^0.2.2"
    }
}
